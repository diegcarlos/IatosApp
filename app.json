{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "icon": "./src/assets/app-icon.png",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#b9bec1",
            "image": "./src/assets/splash-bg.png",
            "resizeMode": "cover"
          },
          "android": {
            "backgroundColor": "#b9bec1",
            "image": "./src/assets/splash-bg.png",
            "resizeMode": "cover"
          }
        }
      ]
    ],
    "name": "Sanabria",
    "slug": "Sanabria",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "44b261a5-ac4e-4cf2-beda-603aae13d9f5"
      }
    },
    "android": {
      "package": "com.diegcarlos.IatosApp",
      "label": "",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
